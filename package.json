{
  "name": "waypoints",
  "version": "5.0.0",
  "author": "Caleb Troughton <caleb@imakewebthings.com> (http://imakewebthings.com)",
  "description": "Easily execute a function when you scroll to an element",
  "scripts": {
    "build": "webpack",
    "lint": "standard",
    "start": "webpack --watch",
    "tdd": "mocha -w -r babel-register --reporter=progress fivetest/*.js",
    "test": "nyc --reporter=html mocha -r babel-register fivetest/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imakewebthings/waypoints.git"
  },
  "keywords": [
    "scroll"
  ],
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "standard": "^8.6.0",
    "webpack": "^1.13.3"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "nyc": {
    "include": ["source/**/*.js"]
  }
}
